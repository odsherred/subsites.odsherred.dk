/**
 * Implements hook_tapir_table_alter().
 */
function agres_bookings_tapir_table_alter(&$table, $table_id) {
  // Modify the display of the cart table.
  
  if ($table_id == 'uc_cart_view_table') {
  $table['#columns'] = array(
    'remove' => array(
      'cell' => t('Remove'),
      'weight' => 0,
    ),
    'image' => array(
      'cell' => t('Products'),
      'weight' => 1,
    ),
    'desc' => array(
      'cell' => '',
      'weight' => 2,
    ),
    'qty' => array(
      'cell' => theme('uc_qty_label'),
      'weight' => 3,
    ),
    'unit_price' => array(
      'cell' => t('Unit Price'),
      'weight' => 5,
    ),    
    'total' => array(
      'cell' => t('Total'),
      'weight' => 6,
    ),
  );

  $subtotal = 0;
